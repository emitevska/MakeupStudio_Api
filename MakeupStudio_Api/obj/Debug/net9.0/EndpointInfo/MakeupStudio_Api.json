          "id": {
            "type": "integer",
            "format": "int32"
          },
          "name": {
            "maxLength": 100,
            "minLength": 0,
            "type": "string"
          },
          "durationMinutes": {
            "maximum": 600,
            "minimum": 1,
            "type": "integer",
            "format": "int32"
          },
          "price": {
            "maximum": 100000,
            "minimum": 0,
